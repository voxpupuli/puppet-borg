{
  "name": "puppet-borg",
  "version": "4.2.1-rc0",
  "author": "Vox Pupuli",
  "summary": "installs, configures, and manages borg",
  "license": "AGPL-3.0",
  "source": "https://github.com/voxpupuli/puppet-borg.git",
  "project_page": "https://github.com/voxpupuli/puppet-borg",
  "issues_url": "https://github.com/voxpupuli/puppet-borg/issues",
  "description": "This is a nice module to manage borg backup jobs",
  "tags": [
    "borg",
    "borgbackup",
    "backup",
    "restore"
  ],
  "dependencies": [
    {
      "name": "saz/ssh",
      "version_requirement": ">= 4.0.0 < 12.0.0"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 2.0.0 < 7.0.0"
    },
    {
      "name": "puppet/systemd",
      "version_requirement": ">= 0.4.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.1 < 10.0.0"
    },
    {
      "name": "puppet/ssh_keygen",
      "version_requirement": ">= 4.1.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Archlinux"
    },
    {
      "operatingsystem": "Gentoo"
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "AlmaLinux",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Rocky",
      "operatingsystemrelease": [
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}
